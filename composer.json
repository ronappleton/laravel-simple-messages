{
    "name": "ronappleton/laravel-simple-messages",
    "description": "A real simple messaging plugin for a Laravel app",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Appleton\\Messages\\": "src/",
            "Tests\\": "tests/",
            "Database\\Factories\\": "database/factories/"
        }
    },
    "authors": [
        {
            "name": "Ron Appleton",
            "email": "ron.appleton@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "laravel/framework": "^11.7",
        "ronappleton/laravel-typed-config": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.15",
        "ronappleton/spatie-laravel-permission-mock": "@dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Appleton\\Messages\\ServiceProvider"
            ]
        }
    }
}
